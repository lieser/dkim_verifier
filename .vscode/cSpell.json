// cSpell Settings
{
    // Version of the setting file.  Always 0.1
    "version": "0.2",
    // language - current active spelling language
    "language": "en,de-de,uk",
    // words - list of words to be always considered correct
    "words": [
        "anonid",
        "atext",
        "AUID",
        "blogspot",
        "camelcase",
        "canonicalization",
        "canonicalize",
        "canonicalized",
        "ccontent",
        "cfws",
        "ctext",
        "DKIM",
        "DMARC",
        "dnssec",
        "endregion",
        "favicons",
        "googlecode",
        "hbox",
        "internalerror",
        "libunbound",
        "lieser",
        "nosig",
        "nsresult",
        "permerror",
        "PERMFAIL",
        "prefs",
        "pseudocode",
        "qcontent",
        "qtext",
        "rcode",
        "rdata",
        "s",
        "SDID",
        "SIGERROR",
        "smtp",
        "softfail",
        "stringbundle",
        "subdomain",
        "subdomains",
        "tagspec",
        "temperror",
        "TEMPFAIL",
        "tooltips",
        "tooltiptext",
        "unittest",
        "vbox",
        "vchar",
        "webextensions"
    ],
    // flagWords - list of words to be always considered incorrect
    // This is useful for offensive words and common spelling errors.
    // For example "hte" should be "the"
    "flagWords": [],
    "ignoreRegExpList": [
        "_domainkey",
        "ALPHADIGITPS",
        "AUTOINSERT_RULE_",
        "USERINSERT_RULE_",
        "DKIMV2",
        "DKIM_DMARCERROR_",
        "DKIM_DMARCERROR_ILLFORMED_",
        "DKIM_DNSERROR_",
        "DKIM_POLICYERROR_",
        "DKIM_POLICYERROR_KEYMISMATCH",
        "DKIM_SIGERROR_KEY_ILLFORMED",
        "DKIM_SIGERROR_KEY_HASHNOTINCLUDED",
        "DKIM_SIGERROR_KEY_NOTEMAILKEY",
        "DKIM_SIGERROR_BADSIG",
        "DKIM_SIGERROR_KEYDECODE",
        "DKIM_SIGERROR_ILLFORMED",
        "DKIM_SIGERROR_NOKEY",
        "DKIM_SIGERROR_KEY_TESTMODE",
        "DKIM_SIGERROR_TIMESTAMPS",
        "DKIM_SIGERROR_TOOLARGE_L",
        "DKIM_SIGWARNING_",
        "DQUOTE",
        "JSDNS",
        "SDIDs",
        // mozilla
        "nsIURI",
        "osfile.jsm",
        // general
        "IntelliSense"
    ],
    "ignorePaths": [
        "data/favicon.json",
        "data/signersDefault.json"
    ]
}
